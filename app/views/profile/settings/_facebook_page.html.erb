<tr>
	<td class="col-md-1 col-sm-1">
		<%= link_to page.link, target:"_blank" do %>
			<img src="<%= page.facebookprofile.url(:thumbnail64)%>" class="profile-thumb-holder cover-profilephoto-holder" style="height:52px;width:52px;">
		<% end %>
	</td>
	<td class="col-md-8 col-sm-8">
		<%= link_to page.name, page.link, target:"_blank", class:"text-grey"%>
	</td>
	<td class="text-center"><i class="fa fa-refresh" disabled="disabled"></i></td>
	<td class="text-center">
		<input type="checkbox" class="js-switch" disabled="disabled" />
	</td>
</tr>