<!-- Page Title -->
<% provide(:title, t('views.utilities.menu.videos')+" - "+t('views.utilities.category.videos.animation')) %>
<!-- Sidebar -->
<% content_for :sidebar do %>
	<% if user_signed_in? %>
		<!-- Signed-in Users-->
		<%= render 'utilities/sidebar/sidebar_profile_signed_in_users'%>
	<% else %>
		<!-- Not signed-in Users-->	
    	<%= render 'utilities/sidebar/sidebar_profile_not_signed_in_users'%>
    <% end %>
<% end %>

<%= render 'explore/categories/videos'%>