<%= f.fields_for :shipping_addresses, :html =>{:id => 'subscription-shipping-address'} do |s| %>
	<div class="row">
		<div class="col-md-12 col-lg-12">
			<!-- Name -->
			<div class="form-group">
				<div class="row mb-20">
					<label class="col-md-3 control-label"><%= t 'views.creator_studio.how_i_get_paid.receiver_name' %>:</label>
					<div class="col-md-9">
						<div class="row">
							<div class="col-md-6">
								<%= s.text_field :name, class:"form-control", placeholder:t('views.creator_studio.how_i_get_paid.receiver_name_placeholder') %>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- Country -->
			<div class="form-group">
				<div class="row mb-20">
					<label class="col-md-3 control-label"><%= t 'views.creator_studio.how_i_pay.country' %>:</label>
					<div class="col-md-9">
						<div class="row">
							<div class="col-md-8">
								<%= s.country_select(:country, { include_blank: t('views.creator_studio.how_i_get_paid.country'),selected: params[:subscription][:shipping_country], except: ["AN"] }, { class: 'simple-select-search required', id: 'verification-country-select',disabled:"disabled" }) %>
							</div>
							<div class="col-md-4">
								<%= s.text_field :city, class:"form-control", placeholder:t('views.creator_studio.how_i_get_paid.city_placeholder') %>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- Address -->
			<div class="form-group">
				<div class="row mb-20">
					<label class="col-md-3 control-label"><%= t 'views.creator_studio.how_i_get_paid.address_line1' %>:</label>
					<div class="col-md-9">
						<%= s.text_field :line1, class:"form-control", placeholder:t('views.creator_studio.how_i_get_paid.address_line1_placeholder') %>
					</div>
				</div>
			</div>
			<!-- Postal code -->
			<div class="form-group">
				<div class="row mb-20">
					<label class="col-md-3 control-label"><%= t 'views.creator_studio.how_i_get_paid.postal_code' %>:</label>
					<div class="col-md-9">
						<div class="row">
							<div class="col-md-3">
								<%= s.text_field :postal_code, class:"form-control", placeholder:t('views.creator_studio.how_i_get_paid.postal_code') %>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
<% end %>