<!-- Initialize Slide -->
<script>
	$(document).ready(function(){
		var owl = $('.owl-carousel');
		owl.owlCarousel({
		    items:1,
		    loop:true,
		    margin:10,
		    autoplay:true,
		    lazyLoad: true,
		    autoplayTimeout:6000,
		    autoWidth:true,
		    autoHeight:true,
		    autoplayHoverPause:true
		});
	});
</script>
<!-- Set up your HTML -->
<div class="owl-carousel" style="width:641px;margin-left:340px;position:absolute;background-color:#cccccc;;">
