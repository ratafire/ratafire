<% if Rails.env.production? %>
	<% if @user.accept_venmo == true then %>
		<%= render "subscriptions/add_card_subscribe_production_venmo"%>
	<% else %>
		<%= render "subscriptions/add_card_subscribe_production"%>
	<% end %>
<% else %>
	<% if @user.accept_venmo == true then %>
		<%= render "subscriptions/add_card_subscribe_development_venmo"%>
	<% else %>
		<%= render "subscriptions/add_card_subscribe_development"%>
	<% end %>
<% end %>