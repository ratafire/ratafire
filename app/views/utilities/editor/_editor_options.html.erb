<div id="editor-options-holder">
    <div class="panel-body">
        <!-- Text Post -->
        <%= link_to new_user_editor_editor_text_path(user.id), method: :get, remote: :true do %>
            <div class="col-xs-2 col-md-2 no-padding animated_hover" data-action="jello">
                <div class="center-block btn btn-float btn-rounded btn-float-lg btn-flat">
                    <i class="huaxia huaxia-pen text-pink-300"></i>
                </div>
            </div>
        <% end %> 
        <!-- Image Post -->
        <%= link_to new_user_editor_editor_image_path(user.id), method: :get, remote: :true do %>
            <div class="col-xs-2 col-md-2 no-padding animated_hover" data-action="jello">
                <div class="center-block btn btn-float btn-rounded btn-float-lg btn-flat">
                    <i class="huaxia huaxia-multipleimage text-brown-300"></i>
                </div>
            </div>
        <% end %>
        <!-- Link Post -->
        <%= link_to new_user_editor_editor_link_path(user.id), method: :get, remote: :true do %>
            <div class="col-xs-2 col-md-2 no-padding animated_hover" data-action="jello">
                <div class="center-block btn btn-float btn-rounded btn-float-lg btn-flat">
                    <i class="huaxia huaxia-link text-green-400"></i>
                </div>
            </div>  
        <% end %>  
        <!-- Audio Post -->
        <%= link_to new_user_editor_editor_audio_path(user.id), method: :get, remote: :true do %>
            <div class="col-xs-2 col-md-2 no-padding animated_hover" data-action="jello">
                <div class="center-block btn btn-float btn-rounded btn-float-lg btn-flat">
                    <i class="huaxia huaxia-music text-info-400"></i>
                </div>
            </div>
        <% end %>        
        <!-- Video Post -->
        <%= link_to new_user_editor_editor_video_path(user.id), method: :get, remote: :true do %>
            <div class="col-xs-2 col-md-2 no-padding animated_hover" data-action="jello">
                <div class="center-block btn btn-float btn-rounded btn-float-lg btn-flat">
                    <i class="huaxia huaxia-jiaocheng text-purple-400"></i>
                </div>
            </div>
        <% end %>             
        <!-- Story Post -->
        <div class="col-xs-2 col-md-2 no-padding" data-popup="tooltip" title='<%= t 'views.utilities.tooltip.coming_soon' %>' data-placement="top">
            <div class="center-block btn btn-float btn-rounded btn-float-lg btn-flat">
                <i class="huaxia huaxia-book text-slate-300"></i>
            </div>
        </div>         
    </div>       
</div>